{% extends 'base.html.twig' %}
{% use 'form.html.twig' %}

{% block content %}
    <h2>Upload Training Certificates</h2>
    {% form_theme form "form_themes/_inline-buttons-theme.html.twig" %}
    {{ block('form_card') }}
{% endblock %}
{% block form_card_body %}
    {{ form_row(form.documents) }}
{% endblock %}
{% block form_card_footer %}
    {{ form_row(form.submit) }}
{% endblock %}

{% block title %}{{ parent() }} | Training Certificate Upload{% endblock %}

{% block breadcrumb %}
    <li class="breadcrumb-item active" aria-current="page">
        Training Certificate Upload
    </li>
{% endblock %}

