{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "ext-ctype": "*",
        "ext-gd": "*",
        "ext-iconv": "*",
        "ext-ldap": "*",
        "ext-pdo": "*",
        "ext-yaml": "*",
        "api-platform/core": "^3.1",
        "babdev/pagerfanta-bundle": "4.x-dev",
        "doctrine/annotations": "^2.0",
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.11",
        "easycorp/easyadmin-bundle": "^4.2",
        "friendsofsymfony/jsrouting-bundle": "^3.2",
        "giggsey/libphonenumber-for-php-lite": "^8.13",
        "gregwar/captcha-bundle": "^2.2",
        "html2text/html2text": "^4.3",
        "league/html-to-markdown": "^5.1",
        "liip/imagine-bundle": "^2.8",
        "nelmio/cors-bundle": "^2.3",
        "pagerfanta/doctrine-orm-adapter": "4.x-dev",
        "pagerfanta/twig": "4.x-dev",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpoffice/phpspreadsheet": "^1.23",
        "phpstan/phpdoc-parser": "^1.2",
        "stof/doctrine-extensions-bundle": "^1.7",
        "symfony/asset": "6.3.*",
        "symfony/console": "6.3.*",
        "symfony/doctrine-messenger": "6.3.*",
        "symfony/dotenv": "6.3.*",
        "symfony/expression-language": "6.3.*",
        "symfony/flex": "^2",
        "symfony/form": "6.3.*",
        "symfony/framework-bundle": "6.3.*",
        "symfony/http-client": "6.3.*",
        "symfony/intl": "6.3.*",
        "symfony/ldap": "6.3.*",
        "symfony/mailer": "6.3.*",
        "symfony/mime": "6.3.*",
        "symfony/monolog-bundle": "^3.0",
        "symfony/notifier": "6.3.*",
        "symfony/process": "6.3.*",
        "symfony/property-access": "6.3.*",
        "symfony/property-info": "6.3.*",
        "symfony/proxy-manager-bridge": "6.3.*",
        "symfony/runtime": "6.3.*",
        "symfony/security-bundle": "6.3.*",
        "symfony/serializer": "6.3.*",
        "symfony/string": "6.3.*",
        "symfony/translation": "6.3.*",
        "symfony/twig-bundle": "6.3.*",
        "symfony/ux-twig-component": "^2.15",
        "symfony/validator": "6.3.*",
        "symfony/web-link": "6.3.*",
        "symfony/webapp-meta": "^1.0",
        "symfony/webpack-encore-bundle": "^1.12",
        "symfony/workflow": "6.3.*",
        "symfony/yaml": "6.3.*",
        "twig/cssinliner-extra": "^3.7",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/html-extra": "^3.4",
        "twig/inky-extra": "^3.7",
        "twig/twig": "^2.12|^3.0",
        "vich/uploader-bundle": "^2.1"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "phpmd": "phpmd phpmd.xml"
    },
    "conflict": {
        "symfony/symfony": "*",
        "doctrine/orm": "2.13.2"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "6.3.*"
        }
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^7.2",
        "deployer/deployer": "^7.3",
        "phpmd/phpmd": "^2.13",
        "phpunit/phpunit": "^9.5",
        "symfony/apache-pack": "^1.0",
        "symfony/browser-kit": "6.3.*",
        "symfony/css-selector": "6.3.*",
        "symfony/debug-bundle": "6.3.*",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "^6.0",
        "symfony/stopwatch": "6.3.*",
        "symfony/var-dumper": "6.3.*",
        "symfony/web-profiler-bundle": "6.3.*"
    }
}
